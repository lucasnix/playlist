<!DOCTYPE html>
<html lang="pt-br">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Playlist BLU</title>
  </head>
  <body>
    <h1>Hello EJS</h1>
    <a href="/admin">Administracao</a>
    <ul>
      <% playlist.forEach((music)=>{ %>
        <section>
        <h2><%= music.name%></h2>
        <p><%= music.author%></p>
        <img src="<%= music.linkImage%>" alt="Imagem da musica">
        <audio controls src="<%= music.linkMusic%>"></audio>
        </section>
      <% }) %>
    </ul>
  </body>
</html>
